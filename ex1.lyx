\begin_layout Standard
נחשב פתרון אנליטי: נגזור את 
\begin_inset Formula $LL\left(\lambda|N_{H},N_{M},N_{B}\right)$
\end_inset

 ונשווה לאפס:
\begin_inset Formula 
\begin{align*}
LL\left(\lambda|N_{H},N_{M},N_{B}\right)' & =\left(N_{H}+N_{M}+2N_{B}\right)\frac{1}{e^{\frac{\lambda}{2}}-1}\cdot e^{\frac{\lambda}{2}}\cdot\frac{1}{2}-M=0\\
\iff & 1+\frac{1}{e^{\frac{\lambda}{2}}-1}=\frac{e^{\frac{\lambda}{2}}}{e^{\frac{\lambda}{2}}-1}=\frac{2M}{N_{H}+N_{M}+2N_{B}}\\
\iff & \frac{1}{e^{\frac{\lambda}{2}}-1}=\frac{2M}{N_{H}+N_{M}+2N_{B}}-1=\frac{2M-N_{H}-N_{M}-2N_{B}}{N_{H}+N_{M}+2N_{B}}\\
\iff & e^{\frac{\lambda}{2}}=\frac{N_{H}+N_{M}+2N_{B}}{2M-N_{H}-N_{M}-2N_{B}}+1\\
\iff & \boxed{\lambda=2\log\left(\frac{N_{H}+N_{M}+2N_{B}}{2M-N_{H}-N_{M}-2N_{B}}+1\right)}
\end{align*}

\end_inset

